{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "type": "tls",
        "tag": "dns_proxy",
        "domain_resolver": "dns_resolver",
        "server": "1.1.1.1"
      },
      {
        "type": "h3",
        "tag": "dns_direct",
        "domain_resolver": "dns_resolver",
        "server": "dns.alidns.com"
      },
      {
        "type": "fakeip",
        "tag": "dns_fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "type": "udp",
        "tag": "dns_resolver",
        "server": "223.5.5.5"
      }
    ],
    "rules": [
      {
        "geosite": "category-ads-all",
        "action": "predefined",  
        "rcode": "NXDOMAIN"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "geosite": "geolocation-!cn",
        "server": "dns_fakeip"
      },
      {
        "geosite": "geolocation-!cn",
        "server": "dns_proxy"
      }
    ],
    "final": "dns_direct",
    "independent_cache": true
  },
  "ntp": {
    "enabled": true,
    "interval": "30m0s",
    "server": "time.apple.com",
    "server_port": 123,
    "detour": "DIRECT"
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 2080
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "auto_route": true,
      "strict_route": true,
      "stack": "mixed",
      "sniff": true,
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126"
    }
  ],
  "route": {
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "dashboard"
    }
  }
}
